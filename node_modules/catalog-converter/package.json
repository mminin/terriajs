{
  "name": "catalog-converter",
  "version": "0.0.9",
  "description": "",
  "keywords": [],
  "main": "dist/catalog-converter.umd.js",
  "module": "dist/catalog-converter.es5.js",
  "typings": "dist/catalog-converter.d.ts",
  "files": [
    "/dist"
  ],
  "author": "Stephen Davies <steve9164@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/TerriaJS/catalog-converter"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=10.0.0"
  },
  "bin": {
    "catalog-converter": "./bin/catalog-converter"
  },
  "scripts": {
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "npm run build:code && typedoc --out docs --target es6 --theme minimal --mode file src",
    "build:code": "tsc --module commonjs && rollup -c rollup.config.js",
    "start": "rollup -c rollup.config.js -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "travis-deploy-once": "travis-deploy-once",
    "prepare": "npm run prebuild && npm run build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-adapter-eslint"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "/src/catalogV7Tools",
      "/src/cli.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^4.1.1",
    "@types/core-js": "^2.5.3",
    "@types/jest": "^25.2.1",
    "@types/json5": "0.0.30",
    "@types/lodash": "^4.14.150",
    "@types/node": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "colors": "^1.3.2",
    "commitizen": "^4.0.4",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "cz-adapter-eslint": "0.3.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "jest-config": "^25.4.0",
    "lodash.camelcase": "^4.3.0",
    "nodemon": "^2.0.4",
    "prettier": "2.0.5",
    "pretty-quick": "^2.0.1",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "rollup": "^2.7.3",
    "rollup-plugin-dts": "1.4.10",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "semantic-release": "^17.2.3",
    "shelljs": "^0.8.3",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^25.4.0",
    "ts-node": "^7.0.1",
    "typedoc": "^0.17.6",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@iadvize-oss/foldable-helpers": "^2.2.0",
    "@iadvize-oss/opaque-type": "^1.0.2-beta.5",
    "@sindresorhus/is": "^2.1.1",
    "core-js": "^3.6.5",
    "json5": "^2.1.3",
    "lodash": "^4.17.15",
    "tslib": "^2.3.1",
    "yargs": "^15.3.1"
  }
}
